{% extends 'base.html.twig' %}

{% block title %}HOME{% endblock %}

{% block body %}
<div class="container">
  {% for type, messages in app.session.flashbag.all() %}
    {% for msg in messages %}
      <div class="alert-home alert alert-{{ type }}">{{ msg }}</div>
    {% endfor %}
  {% endfor %}
</div>
<div id="carouselExampleIndicators" class="carousel slide carousel-fade" data-ride="carousel" data-interval="true">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" src="{{ asset('/img/s1.jpg') }}" alt="First slide">
      <div class="carousel-caption d-none d-md-block">
        <h2 class="h2">Done</h2>
        <p>I finished</p>
      </div>
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="{{ asset('/img/s2.jpg') }}" alt="Second slide">
      <div class="carousel-caption d-none d-md-block">
        <h5>Aplication</h5>
        <p>questionnaire</p>
      </div>
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>


{% endblock %}
{% block javascripts %}{% endblock %}

